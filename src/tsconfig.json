{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["DOM", "ES2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "target": "ES2022",
    "paths": {
      "@internal/*": ["./.vuepress/.temp/internal/*"]
    }
  },
  "include": ["**/.vuepress/**/*", "vuepress.client.ts", "vuepress.config.ts"],
  "exclude": ["node_modules", ".cache", ".temp", "dist"]
}
